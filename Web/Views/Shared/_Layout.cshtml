<!DOCTYPE html>
<html>
<head>
    <title>@ViewData["Title"]</title>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="robots" content="none">
    <meta name="description" content="Vertretungsplan des Gymnasium Unterrieden Sindelfingen">
    <meta name="theme-color" content="#eee">
    <link rel="shortcut icon" type="image/png" href="/images/icon.png">
    <link rel="stylesheet" href="/main.min.css">
    <script src="https://code.jquery.com/jquery-3.2.1.min.js" integrity="sha256-hwg4gsxgFZhOsEEamdOYGBf13FyQuiTwlAQgxVSNgt4=" crossorigin="anonymous"></script>
    <script src="https://use.typekit.net/hii3spj.js"></script>
    <script>try { Typekit.load({ async: true }); } catch (e) { }</script>
    @RenderSection("Scripts", false)
    <script src="/main.min.js"></script>
    <environment names="Production">
        <script>
            (function (i, s, o, g, r, a, m) {
                i['GoogleAnalyticsObject'] = r; i[r] = i[r] || function () {
                    (i[r].q = i[r].q || []).push(arguments)
                }, i[r].l = 1 * new Date(); a = s.createElement(o),
                    m = s.getElementsByTagName(o)[0]; a.async = 1; a.src = g; m.parentNode.insertBefore(a, m)
            })(window, document, 'script', 'https://www.google-analytics.com/analytics.js', 'ga');

            ga('create', 'UA-99091816-1', 'auto');
            ga('send', 'pageview');
        </script>
    </environment>
</head>
<body>
    <main>
        @RenderBody()
    </main>
    @if (!ViewData.ContainsKey("HideFooter"))
    {
        <footer>
            <a href="#" id="jumptotop">&#9650; Nach oben</a>
            <span id="copyright">&copy;2017 <a href="http://dominikstiller.de/" target="_blank">Dominik Stiller</a></span>
        </footer>
    }
</body>
</html>
