{
  // Overwrite values in environment-specific settings (appsettings.Development|Production.json)

  // Production only
  "AWS.Logging": {
    "Region": "AWSLOGGINGREGION",
    "LogGroup": "VertretungsplanServer.Api",
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "DataLoader": {
    "S3Bucket": "S3BUCKET",
    "S3Key": "S3KEY"
  },
  "VertretungsplanController": {
    // Password for POST /dates
    "UpdatePassword": "UPDATEPASSWORD"
  },
  "Notifier": {
    "FCMEnabled": false,
    // Only needed when FCMEnabled = true
    "FCMServerKey": "FCMSERVERKEY"
  }
}
